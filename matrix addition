#include<stdio.h>
int main()
{
 int i,j,a[3][3],b[3][3],c[3][3];
 printf("Enter values for 3X3 matrix.1 => ");
 for(i=0;i<3;i++) 
 {
     for(j=0;j<3;j++){
          scanf("%d",&a[i][j]);
     }
 }
 printf("Enter values for 3X3 matrix.2 => ");
 for(i=0;i<3;i++){

     for(j=0;j<3;j++) {
         scanf("%d",&b[i][j]);
     }
 }
 for(i=0;i<3;i++){

     for(j=0;j<3;j++){

      c[i][j] = a[i][j]+b[i][j];
     }
 }
printf("ADDITION of your entered 2 matrix is =>\n");
for(i=0;i<3;i++)
 {
 for(j=0;j<3;j++)
 {
 printf(" %2d ",c[i][j]);
 }
 printf("\n");
 }
return 0;
}
